@Model ICollection<CustomerListingModel>

    <h1>All Customers</h1>

    <table class="table">
        <tr>
            <th>Name</th>
            <th>Birth Date</th>
            <th>Young Driver</th>
        </tr>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.Name</td>
                <td>@customer.BirthDate</td>
                <td>@customer.IsYoungDriver</td>
            </tr>
        }
    </table>

